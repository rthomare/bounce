{
  "name": "@bounce/webapp",
  "private": true,
  "version": "0.0.0-alpha.1",
  "type": "module",
  "homepage": "https://<your-username>.github.io/<your-repo-name>",
  "scripts": {
    "dev": "vite",
    "add-domain": "echo \"rthree.xyz\" > dist/CNAME",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint",
    "lint:fix": "bun lint --fix",
    "format": "prettier --config ./.prettierrc -w 'src/**/*.{tsx,ts}' && git update-index --again",
    "pretest": "bun lint:fix",
    "test": "vitest --dom",
    "posttest": "npx http-server coverage/lcov-report",
    "test:watch": "bun test --collectCoverage=false --watch",
    "deploy": "./deploy.sh"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "happy-dom": "^17.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^3.0.5"
  }
}
